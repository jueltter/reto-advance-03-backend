{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Console": {
        "FormatterName": "simple",
        "FormatterOptions": {
            "TimestampFormat": "yyyy-MM-dd HH:mm:ss.fff ",
            "IncludeScopes": true
        }
    },
    "AllowedHosts": "*"
}
